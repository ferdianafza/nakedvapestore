<p id="notice"><%= notice %></p>

<h1>Barang Masuks</h1>

<table>
  <thead>
    <tr>
      <th>Stok</th>
      <th>Qty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @barang_masuks.each do |barang_masuk| %>
      <tr>
        <td><%= barang_masuk.stok_id %></td>
        <td><%= barang_masuk.qty %></td>
        <td><%= link_to 'Show', barang_masuk %></td>
        <td><%= link_to 'Edit', edit_barang_masuk_path(barang_masuk) %></td>
        <td><%= link_to 'Destroy', barang_masuk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Barang Masuk', new_barang_masuk_path %>
