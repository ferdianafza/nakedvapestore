<p id="notice"><%= notice %></p>

<h1>Barang Keluars</h1>

<table>
  <thead>
    <tr>
      <th>Stok</th>
      <th>Qty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @barang_keluars.each do |barang_keluar| %>
      <tr>
        <td><%= barang_keluar.stok_id %></td>
        <td><%= barang_keluar.qty %></td>
        <td><%= link_to 'Show', barang_keluar %></td>
        <td><%= link_to 'Edit', edit_barang_keluar_path(barang_keluar) %></td>
        <td><%= link_to 'Destroy', barang_keluar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Barang Keluar', new_barang_keluar_path %>
